@{
    ViewData["Title"] = "Home Page";
}

<header class="bg-dark text-white text-center py-5">
    <h1>Tester</h1>
    <p>Welcome! Let`s complete some tests</p>
</header>

<main class="container py-5">
    <div class="row">

        @foreach (var Test in Model.Tests)
        {
            @Html.DisplayFor(m => Test, "TestCard")
        }
    </div>
</main>